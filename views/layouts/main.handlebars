  <body>
    <nav class="navbar navbar-expand-sm">
      <div class="container-fluid">
        <div class="p-3 col-sm-1">
          <picture>
            <source srcset="..." type="image/svg+xml" />
            <div class="logo">
              <img
                src="/images/Ballot-Box.png"
                class="img-fluid"
                alt="A person putting their ballot into a box."
              />
            </div>
          </picture>
        </div>
        <div class=".navbar-brand container-fluid p-3 col-sm-11">
          <h1>Ballot-Box</h1>
        </div>
      </div>
    <div>
      <div class="nav-link">
        <div class="p-3 row-lg-6">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link active" href="#">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#">Profile</a>
            </li>
            {{#unless loggedIn}}
            <li class="nav-item">
              <a class="nav-link" href="#">Login</a>
            </li>
            {{/unless}}
            {{#if loggedIn}}
            <li class="nav-item">
              <a class="nav-link disabled" href="#">Logout</a>
            </li>
             {{/if}}
          </ul>
        </div>
    </div>
  </nav>
